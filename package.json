{
  "name": "v-form-layer",
  "version": "1.0.5",
  "description": "The form is quickly laid out and validated with layer thinking",
  "keywords": [
    "vue",
    "vuejs",
    "form",
    "focus",
    "layer",
    "validate",
    "layou"
  ],
  "author": "blryli",
  "license": "MIT",
  "main": "dist/v-form-layer.umd.js",
  "module": "dist/v-form-layer.esm.js",
  "unpkg": "dist/v-form-layer.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blryli/v-form-layer.git"
  },
  "bugs": {
    "url": "https://github.com/blryli/v-form-layer/issues"
  },
  "homepage": "https://github.com/blryli/v-form-layer#readme",
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-css": "^4.2.1",
    "cross-env": "^5.2.0",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue-template-compiler": "^2.6.10"
  }
}
